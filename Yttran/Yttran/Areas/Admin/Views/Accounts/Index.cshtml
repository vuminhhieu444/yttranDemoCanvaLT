@model IEnumerable<Yttran.Models.Account>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Accounts</h1>

<p>
    <a href="/Admin/Accounts/Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UsrerName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Password)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreateDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UpdateDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UsrerName)
            </td>
            <td>
                @Html.EditorFor(modelItem => item.Password, new { htmlAttributes = new { type = "password", disabled = "disabled" } })
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreateDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UpdateDate)
            </td>
            <td>
                <a href="/Admin/Accounts/Edit/@item.Id">Edit</a> |
                <a href="/Admin/Accounts/Details/@item.Id">Details</a> |
                <a href="/Admin/Accounts/Delete/@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
